{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "date_day_lookup",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "pivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          year as integer,",
				"          sunday as string,",
				"          monday as string,",
				"          tuesday as string,",
				"          wednesday as string,",
				"          thursday as string,",
				"          friday as string,",
				"          saturday as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 pivot(columnNaming: '$N$V',",
				"     lateral: true) ~> pivot1"
			]
		}
	}
}